<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="colorOnBackgroundLight" format="color" />
    <attr name="colorSurfaceBorder" format="color" />

    <declare-styleable name="MeshToolbar">
        <attr name="toolbarTitle" format="string" />
        <attr name="toolbarTitleColor" format="color" />
        <attr name="toolbarBackground" format="color" />
    </declare-styleable>

    <declare-styleable name="ActionButton">
        <attr name="android:src" format="reference" />
    </declare-styleable>

    <declare-styleable name="TitledTextInput">
        <attr name="title" format="string" />
        <attr name="android:hint" format="string" />
        <attr name="android:inputType" format="enum" />
        <attr name="android:imeOptions" format="enum" />
    </declare-styleable>
</resources>